        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Library_GUI"
        mc:Ignorable="d"
        Title="MSMQ_Messages_Window" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100*"/>
            <ColumnDefinition Width="693*"/>
        </Grid.ColumnDefinitions>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="380" VerticalAlignment="Top" Width="792" FontWeight="Bold" FontFamily="Times New Roman" FontSize="24" FontStyle="Italic" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
